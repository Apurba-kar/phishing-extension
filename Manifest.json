{
    "manifest_version": 3,
    "name": "Phishing Detector",
    "version": "1.0",
    "description": "Warns about potential phishing websites",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    }
}